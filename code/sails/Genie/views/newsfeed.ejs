<!-- Page Heading -->
<section id="portfolio">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2>News Feed</h2> <br>
                <img src="images/sensecorp.png"/ style='width: 30%;'>
                <hr class="star-primary">
            </div>
        </div>
          <row>
          <% _.each(posts, function (post) { %>
            <a href='/fullstory?id=<%=post.id%>'>
              <div class='col-lg-12 Container_Blue'>
            <!-- Project One -->
            <div class="Content col-lg-12">
              <h3 class="col-lg-10"><%= post.Title %></h3>
              <h3 class="Source"><%= post.Source %></h4>
            </div>
            <div>
                <div class="col-lg-12" style="padding: 0px;">

                       <%if (post.ImageURL == '') { %>
                                <img class="img-responsive image-wrap" src="https://www.rivaliq.com/wp-content/uploads/2014/01/nodejs_0.png" alt="">
                            <% } else { %>
                                <img class="img-responsive image-wrap" src="<%=post.ImageURL%>" alt="">
                            <% } %>
                    </div>

                  <div class="col-md-7 Content" style="text-align: center;">



                  </div>
              </div>
          </div>
        </a>
      <% }) %>
    </row>
    <!-- /.row -->
      </div>
    </section>
